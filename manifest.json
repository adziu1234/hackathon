{
  "name": "Dark Mode",
  "description": "Changes the background to a less eye-searing color!",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/yinyang-16.png",
      "32": "images/yinyang-32.png",
      "48": "images/yinyang-48.png",
      "128": "images/yinyang-128.png"
    }
  },
  "icons": {
    "16": "images/yinyang-16.png",
    "32": "images/yinyang-32.png",
    "48": "images/yinyang-48.png",
    "128": "images/yinyang-128.png"
  },
  "options_page": "options.html"
}




